{
  "type": "module",
  "name": "ryanfiller.com",
  "private": true,
  "description": "ryanfiller.com",
  "version": "0.1.0",
  "author": "Ryan Filler <ryanfiller89@gmail.com>",
  "license": "MIT",
  "scripts": {
    "web-components": "rollup --config src/web-components/vite.config.js",
    "dev": "npm run web-components -- -w & vite dev --port 1337",
    "share": "npm run dev -- --host",
    "prepare": "svelte-kit sync",
    "build": "npm run web-components && vite build",
    "_postbuild": "mv build/404/index.html build/404.html",
    "preview": "vite preview",
    "eslint": "eslint . --ext .js,.svelte",
    "stylelint": "stylelint **/*.{css,svelte}",
    "lint": "npm run eslint && npm run stylelint",
    "vitest": "vitest",
    "vitest:coverage": "vitest run --coverage",
    "cy:run": "./node_modules/.bin/cypress run --e2e --browser firefox",
    "cypress": "start-server-and-test dev 1337 cy:run",
    "cypress:watch": "npm run share & cypress open --e2e --browser firefox",
    "test": "npm run dev & ./node_modules/.bin/cypress run --e2e",
    "surge": "ADAPTER=static npm run build && surge --project build --domain beta.ryanfiller.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanfiller/portfolio-svelte"
  },
  "bugs": {
    "url": "https://github.com/ryanfiller/portfolio-svelte/issues"
  },
