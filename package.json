{
	"name": "ryanfiller.com",
	"description": "ryanfiller.com",
	"version": "0.2.0",
	"author": "Ryan Filler <ryanfiller89@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/ryanfiller/portfolio-svelte"
	},
	"bugs": {
		"url": "https://github.com/ryanfiller/portfolio-svelte/issues"
	},
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 1337",
		"share": "npm run dev -- --host",
		"build": "vite build",
		"preview": "vite preview",
		"beta": "ADAPTER=static npm run build -- --mode beta && surge --project build --domain beta.ryanfiller.com",
		"lint:eslint": "prettier --check . && eslint .",
		"lint:stylelint": "stylelint **/*.{css,svelte}",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"format": "prettier --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest --mode testing",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- run & npm run test:e2e"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^1.0.3",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/dom": "^9.3.3",
		"@testing-library/svelte": "^4.0.4",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.45.1",
		"happy-dom": "^9.10.9",
		"postcss": "^8.4.21",
		"postcss-custom-media": "^9.1.2",
		"postcss-html": "^1.5.0",
		"postcss-load-config": "^4.0.1",
		"postcss-nesting": "^11.2.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"stylelint": "^15.2.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard": "^30.0.1",
		"svelte": "^5.0.5",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"vite": "^5.4.4",
		"vitest": "^1.0.0"
	},
	"dependencies": {
		"@tgwf/co2": "^0.13.7",
		"@types/tgwf__co2": "^0.14.2"
	}
}
