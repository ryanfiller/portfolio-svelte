{
	"name": "ryanfiller.com",
	"description": "ryanfiller.com",
	"version": "0.2.0",
	"author": "Ryan Filler <ryanfiller89@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/ryanfiller/portfolio-svelte"
	},
	"bugs": {
		"url": "https://github.com/ryanfiller/portfolio-svelte/issues"
	},
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 1337",
		"share": "npm run dev -- --host",
		"build": "vite build",
		"preview": "vite preview",
		"lint:eslint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:stylelint": "stylelint **/*.{css,svelte}",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"format": "prettier --plugin-search-dir . --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- run & npm run test:e2e"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^1.0.3",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-custom-media": "^9.1.2",
		"postcss-html": "^1.5.0",
		"postcss-load-config": "^4.0.1",
		"postcss-nesting": "^11.2.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"stylelint": "^15.2.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard": "^30.0.1",
		"svelte": "npm:@typhonjs-svelte/svelte@3.55.1-cq",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	}
}
